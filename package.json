{
  "name": "csv2db-importer",
  "version": "1.0.0",
  "description": "CSV to Database Import Tool",
  "type": "commonjs",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "package": "npx pkg ."
  },
  "bin": "src/index.js",
  "pkg": {
    "scripts": "src/**/*.js",
