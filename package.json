      "config.json",
      ".env"
    ],
    "targets": [
      "node18-win-x64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakusanpuwan/csv2db-importer.git"
  },
  "author": "Sakusan Puwanendran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sakusanpuwan/csv2db-importer/issues"
  },
  "homepage": "https://github.com/sakusanpuwan/csv2db-importer#readme",
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "jest": "^29.7.0",
    "knex": "^3.1.0",
    "oracledb": "^6.8.0",
    "pkg": "^5.8.1",
    "winston": "^3.17.0"
  }
}
